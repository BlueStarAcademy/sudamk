{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.gnugo"
  },
  "deploy": {
    "startCommand": "node node_modules/tsx/dist/cli.mjs --tsconfig server/tsconfig.json server/gnugoServer.ts",
    "restartPolicyType": "ON_FAILURE"
  }
}

