{
  "name": "@sudam/game-logic",
  "version": "1.0.0",
  "description": "Shared game logic for SUDAM v2 (Go rules, game modes, etc.)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./src": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sudam/shared": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}

